{
  "name": "@serverless/emulator",
  "version": "0.1.10",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/serverless/emulator"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir -p dist && babel src -d dist --source-maps",
    "lint": "eslint . --cache",
    "test": "jest",
    "watch": "rm -rf dist && mkdir -p dist && babel -w src -d dist --source-maps",
    "start": "npm run build && nodemon dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "sle": "./bin/emulator"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "bluebird": "^3.5.0",
    "decompress": "^4.2.0",
    "fs-extra": "^0.26.7",
    "js-yaml": "^3.6.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-route": "^3.2.0",
    "minimist": "^1.2.0",
    "node-stringify": "^0.2.0",
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
